{
  "name": "latex-math-preview-jupyter",
  "displayName": "Live LaTeX Math for Jupyter Notebooks",
  "description": "Live LaTeX math preview for Markdown cells in Jupyter notebooks",
  "version": "1.0.1",
  "publisher": "MathiasKES",
  "author": {
    "name": "Mathias"
  },
  "categories": ["Visualization", "Notebooks", "Other"],
  "icon": "icon.png",
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/MathiasKES/latex-math-preview-jupyter.git"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "latex-math-preview-jupyter.startPreview",
        "title": "Live LaTeX Math: Start Side Preview"
      }
    ]
  },
  "files": [
    "media/**"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "license": "SEE LICENSE IN REPOSITORY",
  "bugs": {
    "url": "https://github.com/MathiasKES/latex-math-preview-jupyter/issues",
    "email": "brkqj4ruo@mozmail.com"
  },
  "homepage": "https://github.com/MathiasKES/latex-math-preview-jupyter/blob/main/README.md"
}
